@model IEnumerable<A15147442_ENT.Entities.Plan>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Planes</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoDePlan)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoDePlan)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.PlanId }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.PlanId }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.PlanId })
        </td>
    </tr>
}

</table>
