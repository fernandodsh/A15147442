@model IEnumerable<A15147442_ENT.Entities.Contrato>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Contratos</h2>

<p>
    @Html.ActionLink("Crear", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreDeLaEmpresa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaInicial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaFin)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreDeLaEmpresa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaInicial)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaFin)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ContratoId }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.ContratoId }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.ContratoId })
        </td>
    </tr>
}

</table>
